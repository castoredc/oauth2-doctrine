{
    "name": "api-skeletons/oauth2-doctrine",
    "description": "Doctrine OAuth2 Adapter for Laminas Api Tools",
    "license": "MIT",
    "keywords": [
        "oauth2",
        "doctrine",
        "laminas",
        "api-tools"
    ],
    "authors": [
        {
            "name": "Tom H Anderson",
            "email": "tom.h.anderson@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laminas-api-tools/api-tools-oauth2": "^1.8",
        "doctrine/doctrine-orm-module": "^4.1 || ^5.0",
        "laminas-api-tools/api-tools-mvc-auth": "^1.7",
        "laminas/laminas-mvc": "^3.3",
        "laminas/laminas-config": "^3.7",
        "laminas/laminas-modulemanager": "^2.11.0",
        "laminas/laminas-servicemanager": "^3.8",
        "laminas/laminas-i18n": "^2.12"
    },
    "require-dev": {
        "api-skeletons/coding-standard": "^1.0",
        "phpunit/phpunit": "^8.5",
        "laminas/laminas-form": "^3.1.1",
        "laminas/laminas-loader": "^2.8",
        "laminas/laminas-log": "^2.15",
        "laminas/laminas-serializer": "^2.11",
        "laminas/laminas-test": "^4.0",
        "laminas/laminas-authentication": "^2.9",
        "laminas/laminas-eventmanager": "^3.4",
        "mockery/mockery": "^1.3.0",
        "php-coveralls/php-coveralls": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "ApiSkeletons\\OAuth2\\Doctrine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApiSkeletonsTest\\OAuth2\\Doctrine\\": "test/"
        }
    },
    "extra": {
        "laminas": {
            "component": "ApiSkeletons\\OAuth2\\Doctrine",
            "config-provider": "ApiSkeletons\\OAuth2\\Doctrine\\ConfigProvider"
        }
    },
    "suggest": {
        "api-skeletons/oauth2-doctrine-permissions-acl": "Access Control Lists for api-tools-oauth2-doctrine",
        "bushbaby/api-tools-oauth2-doctrine-mutatetablenames": "Allows configuration of the table names that this module uses"
    },
    "config": {
        "sort-packages": true
    }
}
